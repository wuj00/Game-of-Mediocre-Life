<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<% @users.each do |u| %>
  <h2><%= link_to u.username, user_path(u) %></h2>
  <h3>Number of contributed events <%= u.events.count %></h3>
  <hr>
<% end %>

<%= link_to 'Create an account', users_new_path, class: 'btn btn-info' %>
